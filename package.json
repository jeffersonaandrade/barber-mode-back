{
  "name": "lucas-barbearia-backend",
  "version": "1.0.0",
  "description": "Backend do sistema Lucas Barbearia - Gerenciamento de filas para barbearias",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app-fixed-final.js",
    "start:secure": "node scripts/start-secure.js",
    "dev": "nodemon src/app-fixed-final.js",
    "dev:secure": "nodemon --exec 'node scripts/start-secure.js'",
    "dev:permissions": "USE_PERMISSIONS=true nodemon --exec 'node scripts/start.js'",
    "build": "echo 'No build step required for Node.js backend'",
    "vercel-build": "echo 'Vercel build completed'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:services": "jest tests/services/",
    "test:routes": "jest tests/routes/",
    "test:middlewares": "jest tests/middlewares/",
    "test:all": "node tests/run-tests.js",
    "test:mutation": "stryker run",
    "test:mutation:services": "stryker run --mutate src/services/**/*.js",
    "test:mutation:middlewares": "stryker run --mutate src/middlewares/**/*.js",
    "test:mutation:routes": "stryker run --mutate src/routes/**/*.js",
    "test:mutation:all": "stryker run --mutate src/**/*.js",
    "test:quality": "npm run test && npm run test:mutation:services",
    "test:quality:full": "npm run test && npm run test:mutation:all",
    "security:lockfile": "lockfile-lint --path package-lock.json --type npm --allowed-hosts npm --allowed-hosts registry.npmjs.org --allowed-hosts registry.yarnpkg.com",
    "security:audit": "npm audit",
    "security:audit:fix": "npm audit fix",
    "security:check": "node scripts/security-check.js",
    "security:quick": "npm run security:lockfile && npm run security:audit",
    "security:monitor": "npm audit --audit-level=high",
    "security:weekly": "npm audit && npm outdated",
    "security:whatsapp": "npm audit | grep -E '(whatsapp|puppeteer|tar-fs|ws)'",
    "security:whatsapp:monitor": "node scripts/whatsapp-security-monitor.js",
    "preinstall": "npm run security:lockfile",
    "prestart": "npm run security:check"
  },
  "keywords": [
    "barbearia",
    "fila",
    "fastify",
    "supabase"
  ],
  "author": "Lucas Barbearia",
  "license": "MIT",
  "engines": {
    "node": "18.x"
  },
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.0.1",
    "@fastify/helmet": "^13.0.1",
    "@fastify/jwt": "^9.1.0",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@supabase/supabase-js": "^2.38.4",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "fastify": "^5.4.0",
    "groq-sdk": "^0.3.3",
    "joi": "^17.12.0",
    "lockfile-lint": "^4.14.1",
    "moment": "^2.29.4",
    "qrcode": "^1.5.3",
    "qrcode-terminal": "^0.12.0",
    "whatsapp-web.js": "^1.23.0"
  },
  "devDependencies": {
    "@stryker-mutator/core": "^9.0.1",
    "@stryker-mutator/jest-runner": "^9.0.1",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "nodemon": "^3.0.2",
    "tap": "^21.1.0"
  }
}
